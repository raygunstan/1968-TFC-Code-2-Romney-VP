campaignTrail_temp.jet_data = [{}
]


















campaignTrail_temp.candidate_image_url = 'https://media.discordapp.net/attachments/1056049447798312960/1121513271481086062/image.png';
campaignTrail_temp.running_mate_image_url = 'https://media.discordapp.net/attachments/1056049447798312960/1121518751226015884/image.png';
campaignTrail_temp.candidate_last_name = 'Reagan';
campaignTrail_temp.running_mate_last_name = 'Romney';
campaignTrail_temp.running_mate_state_id = '202';
campaignTrail_temp.player_answers = [];
campaignTrail_temp.player_visits = [];
campaignTrail_temp.answer_feedback_flg = 1;
campaignTrail_temp.game_start_logging_id = '3660989';

e = campaignTrail_temp;

construct = (a = 1) => {

   e.page = e.page + a < e.pages.length ? e.page + a : 0;
   let html = e.header;
   html += e.pages[e.page] + "<br>";
   if (e.page > 0) {
     html += `<button onclick='endingConstructor(a = -1)'>Back</button>`;
   }
   if (e.page < e.pages.length - 1) {
     html += `<button onclick='endingConstructor(a = 1)'>Next</button>`;
   }

   if (e.image[e.page] == true)
     setTimeout(()=>{
       candImg = $(".person_image")[0];
       if (candImg) {
         candImg.remove();
         $("#final_results_description")[0].style = `
           text-align:center;
           width: 70%;
           text-overflow: ellipsis;
           display: block;
           margin-left: auto;
           margin-right: auto;
         `;
       }
     }, 10);
   else if (e.image[e.page])
   setTimeout(()=>{
       candImg = $(".person_image")[0];
       if (candImg)
         candImg.src = e.image[e.page];
     }, 10);

   return html;
};

endingConstructor = (a = 1) => {
   $("#final_results_description")[0].innerHTML = construct(a);
};

e.multiple_endings = true;
e.page = 0;
e.initDC = false;

campaignTrail_temp.multiple_endings = true;

endingPicker = (out, totv, aa, quickstats) => {
  let used = false;
  let orderID = [aa[0].candidate, aa[1].candidate, aa[2].candidate];
  let e = campaignTrail_temp;
  let html;

    if (quickstats[0] >= 538) {
      e.image = ["https://i.imgur.com/sxq6f2N.png"];
      e.header = "<h3>Reagan Country</h3>";
      e.pages = ["<p><i>No one dared to ask his business, no one dared to make a slip</p><p>For the stranger there among them had a big iron on his hip</i></p><p>Great stuff, cowboy! You didn’t just beat Humphrey, you absolutely pummeled him! You will enter office with a huge mandate to govern, and if successful, can secure another four years in office come 1972.</p><p>Don’t get too worried, my friend. Presidents get breaks too. You are still more than welcome to return to your Rancho del Cielo and ride your horses with Nancy into the sunset. Hell, why not go and do that now as a sort of celebration?</p><p>Lace up those boots, cowboy! Ain’t much time ‘til January! You’ll have to wear the suit and tie for the bigwigs then.</p>"];
      return construct(0);
    } else if (quickstats[0] >= 330) {
      e.image = ["https://i.imgur.com/TuPKM9q.png"];
      e.header = "<h3>For What It’s Worth</h3>";
      e.pages = ["<p>You sit in your office as the results pour in. Before you know it, Secret Service officials enter your home in California and quickly inform you of national security matters. You’ve just been elected President. It’s happening so quickly, you have no time to think. You glance at the television, with  black and white footage of Walter Cronkite speaking and a chart behind him. You see your name in big letters, REAGAN and numbers next to it, symbolizing electoral votes. You see the large number, maybe 330 or more? Something like that.</p><p>The next day, you dress in your best suit rather quickly, and follow the Secret Service to a small motorcade. As you make your way to the airport, hundreds of supporters line the streets and wave, and you wave back. A smile forms as reality sets in. You will be President.<p/><p>For what it’s worth, Ronnie, you will be President.</p>"];
      return construct(0);
      } else if (quickstats[0] >= 270) {
      e.image = ["https://i.imgur.com/wlBYyya.png"];
      e.header = "<h3>California Dreamin'</h3>";
      e.pages = ["<p>You fought them off. You’ve done it. Governor Reagan, you have been elected President of the United States in a nailbiter. Despite Humphrey’s last minute surge, you have held on and will succeed President Lyndon Johnson.</p><p>You will enter the office with many challenges ahead of you. Economy in the dump, foreign policy challenges, bringing the boys home, etc. But if anybody can do it, it’s most certainly you.<p/><p>Good luck, Mr. President-elect.</p>"];
      return construct(0);
      } else if (quickstats[0] >= 245) {
      e.image = ["https://i.imgur.com/PZKUqg3.png"];
      e.header = "<h3>Ring of Fire</h3>";
      e.pages = ["<p>This was a close one, huh? Despite you holding a solid lead for most of the campaign, Humphrey came back and swooped a narrow victory from the jaws of defeat. I’m sure— no, I know it hurts, being so close to victory yet so far away.</p><p>I bet emotions are very high, and not good emotions. Hubert Humphrey will enter office with a worsening economy and a mucky foreign policy situation. I know you wanted to be the one in that office in January, but maybe you can rest easy in the fact that you won’t have to deal with all the problems Humphrey will have ahead of him.<p/><p>Enjoy the rest of your governorship, Ronnie.</p>"];
      return construct(0);
      } else if (quickstats[0] >= 1) {
      e.image = ["https://i.imgur.com/qlVO8JR.png"];
      e.header = "<h3>Find Solace In Your Efforts, For They Have Not Died In Vain</h3>";
      e.pages = ["<p>Well, you’ve lost. That’s clear as day. But does this mean that conservatism has died? No, not necessarily. Maybe you can be the next Barry Goldwater, and usher in the next candidate for the conservatives, like he did with you. Or, perhaps you can run again sometime in the future and redeem yourself.</p><p>It’s okay to lose, my friend. But it’s not okay to stay a loser. The California governorship is still yours, and there’s no saying you can’t run for a second term. Good luck in your future efforts, Ronnie. We will be watching.</p>"];
      return construct(0);
      } else if (quickstats[0] >= 0) {
      e.image = ["https://i.imgur.com/YaitZZA.png"];
      e.header = "<h3>Paint It Black</h3>";
      e.pages = ["<p>You sit at your desk in your office, alone, for a few hours. How the hell did I lose that bad? You think to yourself as you continue pondering. Conservatism was repudiated once more, and your loss stings just as bad, or worse than Goldwater’s considering the national environment. This was your election to lose.</p><p>Was it bad luck? Were your efforts at sabotage foiled? Have they exposed you? Now, as you gaze up for a moment, you view a wash of flickering red and blue from your windows.<p/><p>They’ve arrived. You begin to gather your documents.</p><p><i>Paint It Black. Paint It Black. Paint It Black. Everything</i></p>"];
      return construct(0);
    }
  
};
